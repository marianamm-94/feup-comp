import io;
class WhileAndIF {

    public static void main(String[] args){
        int a;
        int b;
        int c;
        int[] d;
        a = 20;
        b = 10;
        d = new int[10];


        while((0-1) < c){
            d[c+1] = a-b;
            c= c-1;
            a= a-1;
            b= b-1;
        }
        c=0;
    }

}